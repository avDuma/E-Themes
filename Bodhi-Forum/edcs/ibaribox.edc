         part {
            name: "top_arrow1";
            description {
               state: "default" 0.0;
               aspect: 1.0 1.0;
               aspect_preference: HORIZONTAL;
               rel1.relative: 0.5 -1.0;
               rel2.relative: 0.5 -1.0;
