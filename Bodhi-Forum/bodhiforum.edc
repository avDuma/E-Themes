         image: "exq-dot-glow.png" COMP;
      }
      script {
         public is_ready;
         public do_end;
      }
      parts {
         part {
            name: "base";
            type: RECT;
            description {
               state: "default" 0.0;
               color: 0 0 0 255;
            }
         }
         part {
            name: "light";
            description {
               state: "default" 0.0;
               align: 0.5 0.222222222;
               rel1.relative: 0.104166667 0.515625;
               rel2.relative: 0.895833333 0.796875;
               aspect: 2.111111111 2.111111111;
               aspect_preference: HORIZONTAL;
               max: 380 180;
               image.normal: "exq-bglight.png";
               color: 255 255 255 0;
            }
            description {
               state: "visible" 0.0;
               inherit: "default" 0.0;
               color: 255 255 255 255;
            }
         }
         part {
            name: "logoclip";
