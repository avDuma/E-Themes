style "tree"
{	
	GtkTreeView	::row-ending-details	= 1
	GtkTreeView	::indent-expanders	= TRUE
	GtkTreeView	::allow-rules		= 1
	GtkTreeView	::vertical-separator	= 0

	fg[SELECTED]	= @selected_fg_color
	fg[ACTIVE]	= @selected_fg_color
	text[SELECTED]	= @selected_fg_color
	text[ACTIVE]	= @selected_fg_color
	bg[NORMAL]	= "#3c3c3c"
	base[NORMAL]	= "#c0c0c0"
	engine "pixmap"
	{
		# Active Rows
		#========================================================
		
		# Start Selected images
		image
		{
			function        = FLAT_BOX
			state           = SELECTED
			detail		= "cell_even_start"
			border          = {3, 0, 2, 2}
			file            = "Images/Tree/selected-s.png"
			stretch		= TRUE
		}
		image
		{
			function        = FLAT_BOX
			state           = SELECTED
			detail		= "cell_odd_start"
			border          = {3, 0, 2, 2}
			file            = "Images/Tree/selected-s.png"
			stretch		= TRUE
		}		

