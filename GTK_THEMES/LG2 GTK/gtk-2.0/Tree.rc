style "listheader"
{	GtkButton	::inner-border		= {0,0,0,0}
	font_name = "Bold"

	bg[NORMAL]	= "#ececec"

	fg[NORMAL]	= @fg_selected_tabcolor
	fg[PRELIGHT]	= @fg_selected_tabcolor
	fg[ACTIVE]	= @fg_selected_tabcolor

	engine "pixmap"
	{
		image
		{
			function	= BOX
		}
	}
}
