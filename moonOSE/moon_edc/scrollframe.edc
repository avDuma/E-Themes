	 part { name: "sb_hbar_a1";
	    type: RECT;
	    clip_to: "sb_hbar";
	    mouse_events: 1;
	    description { state: "default" 0.0;
	       min: 17 17;
	       align: 0.0 0.5;
	       aspect: 1.0 1.0;
	       aspect_preference: VERTICAL;
	       color: 0 0 0 0;
	       rel1 {
		  to: "sb_hbar";
		  relative: 0.0  0.0;
