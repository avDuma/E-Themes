	       image.normal: "arrow_down.png";
	    }
	    description { state: "clicked" 0.0;
	       inherit: "default" 0.0;
	       rel1.offset: 0 1;
	       rel2.offset: -1 0;
	    }
	 }

	 part { name: "sb_hbar";
	    type: RECT;
	    mouse_events: 0;
	    scale: 1;
	    description { state: "default" 0.0;
	       min: 17 17;
	       align: 0.0 1.0;
	       rel1 {
		  to: "bg";
		  relative: 0.0 1.0;
