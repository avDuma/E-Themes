/*This file contains the ACTION CODE, what i mean is that these codes
make simple file do some sort of animation*/
      program {
	 name:    "on";
	 signal:  "e,action,show,title";
	 source:  "e";
	 action:  STATE_SET "active" 0.0;
	 target:  "e.text.title";
      }
      program {
	 name:    "off";
	 signal:  "e,action,hide,title";
	 source:  "e";
	 action:  STATE_SET "default" 0.0;
	 target:  "e.text.title";
      }
