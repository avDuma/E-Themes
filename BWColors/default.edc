            }
         }
         part { name: "info_clip";
            type: RECT;
            mouse_events: 0;
            clip_to: "fade_clip";
            description { state: "default" 0.0;
               color: 255 255 255 0;
            }
            description { state: "visible" 0.0;
               color: 255 255 255 255;
            }
         }
         part { name: "pulse_clip";
            type: RECT;
            mouse_events: 0;
            clip_to: "fade_clip";
            description { state: "default" 0.0;
               color: 255 255 255 255;
            }
            description { state: "faded" 1.0;
               color: 255 255 255 255;
            }
            description { state: "faded" 0.0;
               color: 255 255 255 0;
            }
         }
         part { name: "fill_region";
            mouse_events: 0;
            type: RECT;
            description { state: "default" 0.0;
               visible: 0;
               rel1.relative: 0.0 (26/128);
