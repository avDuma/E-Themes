   /* This is put around gadgets in the shelf if they ask for an "inset" style
    * framing. This puts some sort of bordering around them to make them look
    * "inset" into the shelf or screen. this is meant to go with the default
    * shelf style as implied in the group name */
   group {
      name: "e/shelf/default/inset";
      images {
         image: "inset_sunk.png" COMP;
      }
      parts {
         part {
            name: "base";
            type: RECT;
            description {
               state: "default" 0.0;
               rel1 {
                  to: "inset";
                  offset: 1 1;
               }
               rel2 {
                  to: "inset";
                  offset: -2 -2;
               }
               color: 255 255 255 255;
            }
         }
         part {
            name: "clip";
            type: RECT;
            description {
               state: "default" 0.0;
               rel1.to: "base";
               rel2.to: "base";
               color: 255 255 255 255;
            }
         }
         part {
            name: "e.swallow.content";
            type: SWALLOW;
            clip_to: "clip";
            description {
               state: "default" 0.0;
               rel1 {
                  to: "base";
                  offset: 1 1;
               }
               rel2 {
                  to: "base";
                  offset: -2 -2;
               }
            }
         }
         part {
            name: "inset";
            mouse_events: 0;
            description {
