         }                                            \
      }                                               \
   }

	#define BORDERICON2(NAME, FILE)                     \
   group {                                            \
      name: "e/widgets/border/default/"NAME ;         \
      max: 64 64;                                     \
      images.image: FILE COMP;						\
      images.image: "shine/"FILE COMP;				\
      parts {                                         \
         part {                                       \
            name: "icon";                             \
            mouse_events: 0;                         \
            description {                             \
               state: "default" 0.0;                  \
               aspect: 1.0 1.0;                       \
               aspect_preference: BOTH;               \
               image.normal: FILE ;                   \
               color_class: "focus";				\
            }                                         \
         }                                            \
      IMGCOLOR(FILE, "icon", 1 1 1 1, "hide")			\
      }                                               \
   }

