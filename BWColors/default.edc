            target: "error_clip";
            transition: DECELERATE 0.5;
         }
         program {
            name: "stop_invalid";
            action: STATE_SET "default" 0.0;
            target: "error_clip";
            transition: DECELERATE 1.5;
         }
         program {
            name: "select";
            signal: "e,state,selected";
            source: "e";
            action: STATE_SET "visible" 0.0;
            target: "selection";
         }
         program {
            name: "unselect";
            signal: "e,state,unselected";
            source: "e";
            action: STATE_SET "default" 0.0;
            target: "selection";
         }
      }
