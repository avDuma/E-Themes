   #define ICON(NAME, FILE, SIZE)                     \
   group {                                            \
      name: "e/icons/"NAME ;                          \
      max: SIZE SIZE;                                 \
      parts {                                         \
         part {                                       \
            name: "icon";                             \
            repeat_events: 1;                        \
            description {                             \
               state: "default" 0.0;                  \
               aspect: 1.0 1.0;                       \
               aspect_preference: BOTH;               \
               image.image: FILE COMP;                \
               image.normal: FILE ;                   \
            }                                         \
         }                                            \
      }                                               \
   }

	#define ICON2(NAME, FILE, SIZE)						\
	group {                                            \
	  images.image: FILE COMP;                		\
	  images.image: "shine/"FILE COMP;                		\
      name: "e/icons/"NAME ;                          \
      max: SIZE SIZE;                                 \
      parts {                                         \
         part {                                       \
            name: "icon";                             \
            repeat_events: 1;                        \
            description {                             \
               state: "default" 0.0;                  \
               aspect: 1.0 1.0;                       \
               aspect_preference: BOTH;               \
               image.normal: FILE ;                   \
               color_class: "focus";				\
            }                                         \
         }                                            \
	IMGCOLOR(FILE, "icon", 1 1 1 1, "hide")	\
	      }                                               \
   }
   


