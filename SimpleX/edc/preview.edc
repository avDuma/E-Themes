group { name: "e/widgets/preview";
	parts {
		part { name: "clipper"; type: RECT;
			description { state: "default" 0.0;
				rel1.to: "e.swallow.content";
				rel2.to: "e.swallow.content";
			}
		}
		part { name: "e.swallow.content"; type: SWALLOW;
