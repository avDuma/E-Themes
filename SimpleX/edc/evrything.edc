				color: 255 255 255 64;
			}
		}
		part { name: "e.swallow.icons"; type: SWALLOW;
			clip_to: "icon_clip";
			description { state: "default" 0.0;
				min: 48 48;
				max: 48 48;
				aspect: 1.0 1.0; aspect_preference: VERTICAL;
				align: 0.0 0.5;
				rel1.offset: 8 8;
				rel2.offset: 8 -9;
			}
		}
